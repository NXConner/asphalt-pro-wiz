# Build metadata (surfaced in structured logs)
VITE_APP_VERSION=

# Supabase unified project
VITE_SUPABASE_URL=
VITE_SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=
SUPABASE_PROJECT_REF=

# Local Postgres (for node-pg-migrate and seed)
