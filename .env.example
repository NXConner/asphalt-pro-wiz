# Vite client-side API keys (avoid using in production)
VITE_GEMINI_API_KEY=

# Optional: Supabase public client (if used on client)
VITE_SUPABASE_URL=
VITE_SUPABASE_ANON_KEY=

# Server-side environment (used by scripts and backend)
# Prefer using a secrets manager in production.
GEMINI_API_KEY=

# Postgres connection for migrations/seeds (used later)
PGHOST=localhost
PGPORT=5432
PGDATABASE=pavement
PGUSER=postgres
PGPASSWORD=postgres
DATABASE_URL=postgres://postgres:postgres@localhost:5432/pavement

# GitHub token for repo ingestion (scoped: repo, read:org)
GITHUB_TOKEN=

# Admin user email for seeding (must exist or will be created in local DB)
ADMIN_EMAIL=n8ter8@gmail.com
