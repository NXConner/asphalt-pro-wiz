# AI / Observability
# In non-development environments you MUST use VITE_GEMINI_PROXY_URL; direct keys are not allowed
VITE_GEMINI_API_KEY=
# Server-side scripts can use GEMINI_API_KEY as well
GEMINI_API_KEY=
VITE_GEMINI_PROXY_URL=
VITE_LOG_BEACON_URL=
VITE_OBSERVABILITY_EXPORTER_URL=
OBSERVABILITY_API_KEY=
VITE_BASE_PATH=

# Observability / Sentry (optional)
VITE_SENTRY_DSN=
VITE_OBSERVABILITY_SAMPLE_RATE=0.1
VITE_ENABLE_WEB_VITALS=1
VITE_ENABLE_FEATURE_TELEMETRY=1

# RAG ingestion (optional)
GITHUB_TOKEN=

# Feature flags (0/1 or true/false)
