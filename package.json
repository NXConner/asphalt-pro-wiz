  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:mobile": "vite build --base ./",
    "build:dev": "vite build --mode development",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "audit": "npm audit --audit-level=high",
    "snyk:test": "snyk test || true",
    "snyk:monitor": "snyk monitor || true",
    "prepare": "husky",
    "preview": "vite preview",
    "ingest:repos": "tsx scripts/ingest/ingest.ts",
    "migrate:create": "node-pg-migrate -m supabase/migrations create",
    "migrate:up": "node-pg-migrate -m supabase/migrations -d \"$DATABASE_URL\" up",
    "migrate:down": "node-pg-migrate -m supabase/migrations -d \"$DATABASE_URL\" down",
    "seed": "tsx scripts/seed.ts",
