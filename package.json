    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "coverage": "vitest run --coverage",
    "cap:init": "cap init --web-dir=dist \"Pavement Performance Suite\" com.pavement.performance",
    "cap:add:android": "cap add android",
    "cap:sync": "cap sync",
    "mobile:prep": "npm run build:mobile && cap copy android && cap sync android",
    "android:gradle:debug": "android/gradlew assembleDebug",
    "android:gradle:release": "android/gradlew assembleRelease",
    "android:debug": "npm run mobile:prep && npm run android:gradle:debug",
    "android:release": "npm run mobile:prep && npm run android:gradle:release",
    "android:open": "cap open android"
  },
